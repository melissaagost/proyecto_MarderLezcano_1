<Window x:Class="proyecto_MarderLezcano.Views.ListadoCitas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proyecto_MarderLezcano.Views"
        mc:Ignorable="d"
        Title="Citas programadas" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Views/Tu salud, nuestra prioridad(4).png"/>
        </Grid.Background>
        <Rectangle x:Name="RAgendarCita" HorizontalAlignment="Left" Height="352" Margin="27,0,0,0" VerticalAlignment="Center" Width="373" Fill="#FFE8E5D3"/>
        <TextBlock x:Name="TBAgendarCita" HorizontalAlignment="Left" Margin="45,59,0,0" TextWrapping="Wrap" Text="Programe una nueva cita" VerticalAlignment="Top" Background="#FF60A67C" FontFamily="Verdana" FontWeight="Bold" FontSize="16" Foreground="#FFFFFDF0" Padding="5"/>

    </Grid>
</Window>
